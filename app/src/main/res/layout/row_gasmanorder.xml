<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="gasmanorderRowModel" type="com.revomvpandriodapp.app.modules.homedashb.data.model.GasmanorderRowModel"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumniconfortynine"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_1">
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowiconfortynine"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_15pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_20pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <ImageView android:id="@+id/imageIconFortyNine"
                android:layout_height="@dimen/_25pxh"
                android:layout_marginBottom="@dimen/_4pxv"
                android:layout_marginStart="@dimen/_7pxh"
                android:layout_marginTop="@dimen/_6pxv"
                android:layout_width="@dimen/_25pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_youngwomansit"
                tools:ignore="ContentDescription" tools:src="@drawable/img_youngwomansit"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnweight"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_2pxv"
                android:layout_marginStart="@dimen/_4pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_weight="0.53" android:layout_width="0dp" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtWeight"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{gasmanorderRowModel.txtWeight}"
                    style="@style/txtRobotoromanregular15" tools:text="@string/lbl_12kg2"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPendingTextFor"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{gasmanorderRowModel.txtPendingTextFor}"
                    style="@style/txtRobotoromanregular10_1" tools:text="@string/lbl_pending"/>
            </LinearLayout>
            <TextView android:drawablePadding="@dimen/_13pxh"
                android:drawableStart="@drawable/img_calendar"
                android:ellipsize="end" android:id="@+id/txtFrame247"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_4pxv"
                android:layout_marginStart="@dimen/_4pxh"
                android:layout_marginTop="@dimen/_4pxv"
                android:layout_weight="0.39" android:layout_width="0dp"
                android:paddingBottom="@dimen/_5pxv"
                android:paddingEnd="@dimen/_21pxh"
                android:paddingStart="@dimen/_9pxh"
                android:paddingTop="@dimen/_5pxv"
                android:singleLine="true"
                android:text="@{gasmanorderRowModel.txtFrame247}"
                style="@style/txtSolidRounded" tools:text="@string/lbl_2023"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearRowdetails"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <LinearLayout android:id="@+id/linearRow12kgtextformfi"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_3pxv"
                android:layout_marginTop="@dimen/_3pxv"
                android:layout_weight="0.29" android:layout_width="0dp" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txt12KgTextFormFi"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_weight="0.34"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{gasmanorderRowModel.txt12KgTextFormFi}"
                    style="@style/txtRobotoromanregular17" tools:text="@string/lbl_n"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txt12KgTextFormFiOne"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.66"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{gasmanorderRowModel.txt12KgTextFormFiOne}"
                    style="@style/txtRobotoromanregular17" tools:text="@string/lbl_0_0"/>
            </LinearLayout>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnDetails"
                android:layout_height="@dimen/_27pxv"
                android:layout_marginStart="@dimen/_161pxh"
                android:layout_width="@dimen/_100pxh"
                android:paddingBottom="@dimen/_5pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_5pxv"
                android:singleLine="true"
                android:text="@string/lbl_details"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_1" tools:text="@string/lbl_details"/>
        </LinearLayout>
    </LinearLayout>
</layout>
