<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="householddashboardHomeContainerVM" type="com.revomvpandriodapp.app.modules.householddashboardhomecontainer.data.viewmodel.HouseholddashboardHomeContainerVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumnTwo"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/_1pxv"
        android:layout_marginTop="@dimen/_4pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <FrameLayout android:id="@+id/fragmentContainer"
            android:layout_height="match_parent"
            android:layout_weight="1" android:layout_width="match_parent"/>
        <FrameLayout android:gravity="start"
            android:id="@+id/frameBottomappbarcombinedsha"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_51pxv" android:layout_width="match_parent">
            <FrameLayout android:id="@+id/frameStackcombinedshape"
                android:layout_height="@dimen/_95pxv" android:layout_width="match_parent">
                <ImageView android:id="@+id/imageCombinedShape"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_95pxh"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_combinedshape"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_combinedshape"/>
                <LinearLayout android:id="@+id/linearRowhome"
                    android:layout_gravity="top|start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_32pxh"
                    android:layout_marginStart="@dimen/_32pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumnhome"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_22pxv"
                        android:layout_weight="0.28"
                        android:layout_width="0dp" android:orientation="vertical">
                        <FrameLayout android:id="@+id/frameHome"
                            android:layout_height="@dimen/_24pxv"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginStart="@dimen/_1pxh"
                            android:layout_width="@dimen/_24pxh" style="@style/groupStyle">
                            <ImageView android:id="@+id/imageHome"
                                android:layout_gravity="center"
                                android:layout_height="@dimen/_20pxh"
                                android:layout_marginHorizontal="@dimen/_2pxh"
                                android:layout_marginVertical="@dimen/_2pxv"
                                android:layout_width="@dimen/_19pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_home"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_home"/>
                        </FrameLayout>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtHomeOne"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_6pxv"
                            android:layout_width="wrap_content"
                            android:letterSpacing="0.01"
                            android:singleLine="true"
                            android:text="@{householddashboardHomeContainerVM.householddashboardHomeContainerModel.txtHomeOne}"
                            style="@style/txtRobotoromanregular10" tools:text="@string/lbl_home"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumnvolume"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_25pxv"
                        android:layout_weight="0.24"
                        android:layout_width="0dp" android:orientation="vertical">
                        <ImageView android:id="@+id/imageVolume"
                            android:layout_height="@dimen/_20pxh"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginStart="@dimen/_3pxh"
                            android:layout_width="@dimen/_20pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_volume"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_volume"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtExpenses"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_7pxv"
                            android:layout_width="wrap_content"
                            android:letterSpacing="0.01"
                            android:singleLine="true"
                            android:text="@{householddashboardHomeContainerVM.householddashboardHomeContainerModel.txtExpenses}"
                            style="@style/txtRobotoromanregular10" tools:text="@string/lbl_orders"/>
                    </LinearLayout>

                    <com.google.android.material.floatingactionbutton.FloatingActionButton
                        android:backgroundTint="@color/blue_A400"
                        android:id="@+id/floatingBtnFloatingactionbutton"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_17pxv"
                        android:layout_weight="0.18"
                        android:layout_width="0dp"
                        app:srcCompat="@drawable/img_plus" app:tint="@color/white_A700"/>
                    <LinearLayout android:gravity="center_horizontal"
                        android:id="@+id/linearColumnmusic"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_21pxh"
                        android:layout_marginTop="@dimen/_25pxv"
                        android:layout_weight="0.2"
                        android:layout_width="0dp" android:orientation="vertical">
                        <ImageView android:id="@+id/imageMusic"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_20pxh"
                            android:layout_marginEnd="@dimen/_24pxh"
                            android:layout_marginStart="@dimen/_9pxh"
                            android:layout_width="@dimen/_20pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_music"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_music"/>
                        <TextView android:ellipsize="end"
                            android:gravity="start"
                            android:id="@+id/txtWallet"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_17pxh"
                            android:layout_marginTop="@dimen/_8pxv"
                            android:layout_width="match_parent"
                            android:letterSpacing="0.01"
                            android:singleLine="true"
                            android:text="@{householddashboardHomeContainerVM.householddashboardHomeContainerModel.txtWallet}"
                            style="@style/txtRobotoromanregular10" tools:text="@string/lbl_support"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="center_horizontal"
                        android:id="@+id/linearColumnarrowright"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_21pxh"
                        android:layout_marginTop="@dimen/_25pxv"
                        android:layout_weight="0.09"
                        android:layout_width="0dp" android:orientation="vertical">
                        <ImageView android:id="@+id/imageArrowright"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="@dimen/_19pxh"
                            android:layout_marginEnd="@dimen/_3pxh"
                            android:layout_marginStart="@dimen/_3pxh"
                            android:layout_width="@dimen/_18pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_arrowright_indigo_900"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright_indigo_900"/>
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtProfile"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_8pxv"
                            android:layout_width="match_parent"
                            android:letterSpacing="0.01"
                            android:singleLine="true"
                            android:text="@{householddashboardHomeContainerVM.householddashboardHomeContainerModel.txtProfile}"
                            style="@style/txtRobotoromanregular10" tools:text="@string/lbl_menu"/>
                    </LinearLayout>
                </LinearLayout>
            </FrameLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
