<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="gasmanordersRowModel" type="com.revomvpandriodapp.app.modules.businessdashboard.data.model.GasmanordersRowModel"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumniconfortynineOne"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_1">
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowiconfortynineOne"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_20pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <FrameLayout android:id="@+id/frameIcbaselineapp"
                android:layout_height="@dimen/_36pxv"
                android:layout_width="@dimen/_32pxh" style="@style/groupStylegray_100cornerRadius">
                <ImageView android:id="@+id/imageIconFortyNineOne"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_25pxh"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginEnd="@dimen/_4pxh"
                    android:layout_marginStart="@dimen/_4pxh"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="@dimen/_23pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_youngwomansit"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_youngwomansit"/>
            </FrameLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnrevogastextfoOne"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_2pxv"
                android:layout_marginStart="@dimen/_12pxh"
                android:layout_marginTop="@dimen/_2pxv"
                android:layout_weight="0.88" android:layout_width="0dp" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtRevoGasTextFoOne"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{gasmanordersRowModel.txtRevoGasTextFoOne}"
                    style="@style/txtRobotoromanregular15" tools:text="@string/lbl_revo_gas"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPendingTextForOne"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{gasmanordersRowModel.txtPendingTextForOne}"
                    style="@style/txtRobotoromanregular10_1" tools:text="@string/lbl_pending"/>
            </LinearLayout>
            <ImageView android:id="@+id/imageMoreverticalOne"
                android:layout_height="@dimen/_10pxh"
                android:layout_marginBottom="@dimen/_22pxv"
                android:layout_marginEnd="@dimen/_7pxh"
                android:layout_marginStart="@dimen/_12pxh"
                android:layout_marginTop="@dimen/_2pxv"
                android:layout_width="@dimen/_1pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_morevertical"
                tools:ignore="ContentDescription" tools:src="@drawable/img_morevertical"/>
        </LinearLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowweight"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_20pxv"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <LinearLayout android:id="@+id/linearRowcalendar"
                android:layout_height="wrap_content"
                android:layout_weight="0.73" android:layout_width="0dp"
                android:orientation="horizontal" style="@style/groupStylewhite_2">
                <ImageView android:id="@+id/imageCalendarOne"
                    android:layout_height="@dimen/_11pxh"
                    android:layout_marginBottom="@dimen/_7pxv"
                    android:layout_marginStart="@dimen/_9pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_width="@dimen/_10pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_calendar"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_calendar"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txt10042023TextOne"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginEnd="@dimen/_8pxh"
                    android:layout_marginStart="@dimen/_13pxh"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_weight="0.86"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{gasmanordersRowModel.txt10042023TextOne}"
                    style="@style/txtRobotoromanmedium13" tools:text="@string/lbl_10_04_2023"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtWeightOne"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginStart="@dimen/_5pxh"
                android:layout_marginTop="@dimen/_5pxv"
                android:layout_weight="0.27" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{gasmanordersRowModel.txtWeightOne}"
                style="@style/txtRobotoromanregular17" tools:text="@string/lbl_12kg"/>
        </LinearLayout>
        <androidx.appcompat.widget.AppCompatButton
            android:ellipsize="end" android:gravity="center"
            android:id="@+id/btnDetailsOne"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="@dimen/_20pxh"
            android:layout_marginVertical="@dimen/_20pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_5pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_5pxv" android:singleLine="true"
            android:text="@string/lbl_details"
            android:textAllCaps="false" style="@style/btnSolidRounded_1" tools:text="@string/lbl_details"/>
    </LinearLayout>
</layout>
