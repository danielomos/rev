<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="gasmancontinerVM" type="com.revomvpandriodapp.app.modules.gasmancontiner.data.viewmodel.GasmancontinerVM"/>
    </data>
    <LinearLayout android:id="@+id/linearColumnellipsenineteen"
        android:layout_height="match_parent"
        android:layout_marginEnd="@dimen/_15pxh"
        android:layout_marginStart="@dimen/_7pxh"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearRowellipsenineteen"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_marginStart="@dimen/_8pxh"
            android:layout_width="match_parent" android:orientation="horizontal">
            <View
                android:background="@drawable/rectangle_bg_white_a700_border_gray_101_radius_24_795"
                android:id="@+id/viewEllipseNineteen"
                android:layout_height="@dimen/_45pxv" android:layout_width="@dimen/_49pxh"/>
            <ImageView android:id="@+id/imageEllipseTwenty"
                android:layout_height="@dimen/_45pxh"
                android:layout_marginStart="@dimen/_242pxh"
                android:layout_width="@dimen/_49pxh"
                android:scaleType="centerCrop" circular="@{true}"
                imageRes="@{@drawable/img_ellipse20}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse20"/>
        </LinearLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowhello"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_marginStart="@dimen/_2pxh"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <TextView android:ellipsize="end" android:id="@+id/txtHello"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_weight="0.34" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{gasmancontinerVM.gasmancontinerModel.txtHello}"
                style="@style/txtRobotoromanmedium24" tools:text="@string/lbl_hello"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtRevogasTextFor"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_weight="0.51" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{gasmancontinerVM.gasmancontinerModel.txtRevogasTextFor}"
                style="@style/txtRobotoromanmedium24" tools:text="@string/lbl_revogas2"/>
            <ImageView android:id="@+id/imageNotowavinghan"
                android:layout_height="@dimen/_25pxh"
                android:layout_marginBottom="@dimen/_3pxv"
                android:layout_marginEnd="@dimen/_1pxh"
                android:layout_marginStart="@dimen/_6pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="@dimen/_25pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_notowavinghan"
                tools:ignore="ContentDescription" tools:src="@drawable/img_notowavinghan"/>
        </LinearLayout>
        <TextView android:ellipsize="end" android:gravity="start"
            android:id="@+id/txtDeliverCooking"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_marginTop="@dimen/_3pxv"
            android:layout_width="match_parent"
            android:singleLine="true"
            android:text="@{gasmancontinerVM.gasmancontinerModel.txtDeliverCooking}"
            style="@style/txtRobotoromanregular17" tools:text="@string/msg_deliver_cooking"/>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumngroup237"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_9pxh"
            android:layout_marginStart="@dimen/_10pxh"
            android:layout_marginTop="@dimen/_14pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylecornerRadius">
            <LinearLayout android:id="@+id/linearRowstocklevel"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linearRowgroup237"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.34"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <ImageButton
                        android:background="@drawable/rectangle_bg_white_a700_radius_19"
                        android:id="@+id/btnGroup237"
                        android:layout_height="@dimen/_38pxh"
                        android:layout_width="@dimen/_38pxh"
                        android:paddingHorizontal="@dimen/_6pxh"
                        android:paddingVertical="@dimen/_6pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_youngwomansit" tools:ignore="ContentDescription"/>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumnretailertextfo"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_9pxh"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_weight="0.6"
                        android:layout_width="0dp" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtRetailerTextFo"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{gasmancontinerVM.gasmancontinerModel.txtRetailerTextFo}"
                            style="@style/txtRobotoromanmedium17_1" tools:text="@string/lbl_retailer"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtSeriaIdTextFor"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_9pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{gasmancontinerVM.gasmancontinerModel.txtSeriaIdTextFor}"
                            style="@style/txtRobotoromanregular10_4" tools:text="@string/lbl_ra_300000"/>
                    </LinearLayout>
                </LinearLayout>
                <ProgressBar android:id="@+id/progressBarStocklevel"
                    android:indeterminate="false"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_4pxv"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_weight="0.66"
                    android:layout_width="0dp" android:max="100"
                    android:minHeight="@dimen/_30pxv"
                    android:progressDrawable="@drawable/layer_list_border_black_900_7f_radius_10" style="@style/Widget.AppCompat.ProgressBar.Horizontal"/>
            </LinearLayout>
            <androidx.recyclerview.widget.RecyclerView
                android:gravity="center_horizontal"
                android:id="@+id/recyclerListsellingPrice"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_20pxv"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_136pxh}" tools:itemCount="2" tools:listitem="@layout/row_listselling_price"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearRowsettings"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_marginStart="@dimen/_10pxh"
            android:layout_marginTop="@dimen/_13pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumnsettings"
                android:layout_height="wrap_content"
                android:layout_weight="0.5" android:layout_width="0dp"
                android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
                <ImageView android:id="@+id/imageSettings"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_22pxh"
                    android:layout_marginEnd="@dimen/_11pxh"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_width="@dimen/_22pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_settings"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_settings"/>
                <TextView android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtQuestionsabout"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_marginEnd="@dimen/_11pxh"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{gasmancontinerVM.gasmancontinerModel.txtQuestionsabout}"
                    style="@style/txtRobotoromanregular15_1" tools:text="@string/lbl_start_selling"/>
            </LinearLayout>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumnsettingsOne"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_41pxh"
                android:layout_weight="0.5" android:layout_width="0dp"
                android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
                <ImageView android:id="@+id/imageSettingsOne"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_22pxh"
                    android:layout_marginEnd="@dimen/_11pxh"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_width="@dimen/_22pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_settings"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_settings"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtQuestionsaboutOne"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_13pxv"
                    android:layout_marginEnd="@dimen/_11pxh"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{gasmancontinerVM.gasmancontinerModel.txtQuestionsaboutOne}"
                    style="@style/txtRobotoromanregular15_1" tools:text="@string/lbl_wallet"/>
            </LinearLayout>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameStacklanguage"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_marginStart="@dimen/_10pxh"
            android:layout_marginTop="@dimen/_4pxv"
            android:layout_width="@dimen/_295pxh" style="@style/groupStylewhite_A700">
            <HorizontalScrollView android:fillViewport="true"
                android:id="@+id/horizontalScrollHorizontalscrollcreatefromfra"
                android:layout_gravity="top|start"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="match_parent" android:scrollbars="none">
                <FrameLayout android:id="@+id/frameStackcreatefromfra"
                    android:layout_height="@dimen/_18pxv" android:layout_width="@dimen/_295pxh">
                    <View android:background="@color/white_A700"
                        android:id="@+id/viewCreatefromfra"
                        android:layout_gravity="end"
                        android:layout_height="@dimen/_18pxv" android:layout_width="@dimen/_376pxh"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtShowall"
                        android:layout_gravity="bottom|end"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{gasmancontinerVM.gasmancontinerModel.txtShowall}"
                        style="@style/txtRobotoromanregular13" tools:text="@string/lbl_show_all"/>
                </FrameLayout>
            </HorizontalScrollView>
            <TextView android:ellipsize="end"
                android:id="@+id/txtLanguage"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{gasmancontinerVM.gasmancontinerModel.txtLanguage}"
                style="@style/txtRobotoromanmedium17_1" tools:text="@string/lbl_recent_orders"/>
        </FrameLayout>
        <androidx.recyclerview.widget.RecyclerView
            android:gravity="center_horizontal"
            android:id="@+id/recyclerGasmanorder"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_10pxh"
            android:layout_marginTop="@dimen/_9pxv"
            android:layout_width="match_parent"
            android:orientation="vertical"
            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
            tools:itemCount="3" tools:listitem="@layout/row_gasmanorder"/>
    </LinearLayout>
</layout>
